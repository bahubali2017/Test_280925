# Anamnesis Medical AI Assistant - Environment Variables
# Copy this file to .env and fill in the values with your actual secrets

# Node Environment
NODE_ENV=development

# Supabase Configuration - Backend Variables
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Supabase Configuration - Frontend Variables (Vite)
# These are prefixed with VITE_ to make them available to the frontend
VITE_SUPABASE_URL=https://your-project-ref.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# API Configuration
VITE_API_URL=http://localhost:5000
API_URL=http://localhost:5000

# Deep Seek AI Configuration (for chat functionality)
DEEPSEEK_API_KEY=your-deepseek-api-key-here
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_MAX_TOKENS=2048
DEEPSEEK_TEMPERATURE=0.7
