  "validationRules": {
    "required_fields": ["triageLevel", "isHighRisk", "shouldHaveDisclaimer"],
    "triage_levels": ["emergency", "urgent", "non_urgent"],
    "processing_time_threshold_ms": 1000,
    "emergency_recall_threshold": 1.0,
    "urgent_precision_threshold": 0.85,
    "disclaimer_coverage_threshold": 0.9
  },
  "regressionBaselines": {
    "emergency_detection_rate": 1.0,
    "urgent_detection_rate": 0.95,
    "false_positive_rate": 0.05,
    "average_processing_time_ms": 150,
    "disclaimer_coverage": 0.95
  }
